(define (animation template)
    
    ; -------------------------------------------------------------------------;
    
    (:predicate position
        :parameters (?p)
        :effect (
            (assign 
                (?p x y) 
                (function distribute_grid_around_point (objects ?p))
            )
            :priority 0
        )
    )
    
    (:predicate at
        :parameters (?b ?p)
        :effect (
            (equal (?b x) (?p x))
            (equal (?b y) (?p y))
            :priority 0
        )
    )
    
    
    ; -------------------------------------------------------------------------;
    
    (:visual mov
        :type predefine
        :objects %mov
        :properties (
            (prefabImage img-circle)
            (showName FALSE)
            (x Null)
            (y Null)
            (color #393E41)
            (width 100)
            (height 100)
        )
    )
    
    (:visual pos
        :type predefine
        :objects %pos
        :properties (
            (prefabImage img-square)
            (showName TRUE)
            (x Null)
            (y Null)
            (color #F6F7EB)
            (width 100)
            (height 100)
        )
    )
    
    (:visual tile
        :type predefine
        :objects %tile
        :properties (
            (prefabImage img-square)
            (showName FALSE)
            (x Null)
            (y Null)
            (color #E94F37)
            (width 100)
            (height 100)
        )
    )

    ; -------------------------------------------------------------------------;
    
    (:image 
        (img-square )
        (img-circle )
    )
)